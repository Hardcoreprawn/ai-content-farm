{
    "bindings": [
        {
            "name": "inputBlob",
            "type": "blobTrigger",
            "direction": "in",
            "path": "hot-topics/{name}",
            "connection": "AzureWebJobsStorage"
        },
        {
            "name": "outputBlob",
            "type": "blob",
            "direction": "out",
            "path": "content-pipeline/ranked-topics/ranked_{DateTime}.json",
            "connection": "AzureWebJobsStorage"
        }
    ]
}